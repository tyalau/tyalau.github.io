{
  "printWidth": 130,
  "trailingComma": "es5",
  "semi": false,
  "singleQuote": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "^react$",
    "<THIRD_PARTY_MODULES>",
    "^@/constants/",
    "^@/types/",
    "^@/utils/",
    "^@/lib/",
    "^@/hooks/",
    "^@/services/",
    "^@/components/",
    "^@/containers/",
    "^@/assets/",
    "^@/(?!assets/).*",
    "^[../]",
    "^[./]"
  ],
  "importOrderCaseInsensitive": true
}
